from flask import Flask

app = Flask(__name__)

@app.route('/', methods=['Get'])
def hello_world():
        return "Hello World!!\n"

@app.route('/<name>', methods=['Get'])
def hello_world(name):
	return "Hello {}\n".format(name)

if __name__ == '__main__':
        app.run(debug=True, host='0.0.0.0')
